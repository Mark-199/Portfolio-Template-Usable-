# Example environment variables for Django project

# Set to 'True' for development, 'False' for production
DEBUG=True

# A strong, unique secret key for Django. GENERATE A NEW ONE FOR PRODUCTION!
DJANGO_SECRET_KEY='your-secret-key-here'

# Comma-separated list of hosts that this Django site can serve.
# Example: .localhost, .yourdomain.com
DJANGO_ALLOWED_HOSTS='localhost,127.0.0.1'

# Comma-separated list of trusted origins for CSRF protection.
# Example: https://yourdomain.com
CSRF_TRUSTED_ORIGINS=

# URL for the Django admin interface.
DJANGO_ADMIN_URL='admin/'

# Database settings (for PostgreSQL example)
DB_NAME='your_database_name'
DB_USER='your_database_user'
DB_PASSWORD='your_database_password'
DB_HOST='localhost'
DB_PORT='5432'
